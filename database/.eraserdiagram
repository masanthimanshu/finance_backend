entity-relationship-diagram

user [icon: user, color: teal] {
    id string pk
    name string
    code string
    phone number
    isVerified boolean
}

otp [icon: code, color: green] {
    id string pk
    user string
    value int
}

profile [icon: chart, color: purple] {
    id string pk
    user string
    income number
    expense number
    savings number
    investment number
}

chat [icon: ticket, color: navy] {
    id string pk
    input string
    user string
    amount number
    category string
    subCategory string
    time timestamp
}

user.id <> otp.user
chat.user <> user.id
profile.user <> user.id
